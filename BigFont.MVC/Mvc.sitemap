<?xml version="1.0" encoding="utf-8" ?>
<mvcSiteMap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="http://mvcsitemap.codeplex.com/schemas/MvcSiteMap-File-3.0"
            xsi:schemaLocation="http://mvcsitemap.codeplex.com/schemas/MvcSiteMap-File-3.0 MvcSiteMapSchema.xsd"
            enableLocalization="true">

  <mvcSiteMapNode title="Home" Controller="Home" action="Index" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-home">
    <mvcSiteMapNode title="People" action="People" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-smile">
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Products" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-heart" clickable="false">
      <mvcSiteMapNode title="Services" action="Services" changeFrequency="Always" updatePriority="Normal"  imageUrl="icon-briefcase">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Packages" action="Packages" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-tag">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Presentations" action="Presentations" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-calendar">
      </mvcSiteMapNode>
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Resources" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bolt" clickable="false">
      <mvcSiteMapNode title="Community" action="Community" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-globe">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Checklists" action="Checklists" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-check-empty">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Links" action="Links" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-external-link-sign">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Analytics" action="Analytics" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bar-chart">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Demo Shop" action="Shop" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-shopping-cart" >
      </mvcSiteMapNode>
      <mvcSiteMapNode title="SiteMap" action="SiteMap" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-sitemap">
      </mvcSiteMapNode>
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Contact" action="Contact" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bell">
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Account" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-key" clickable="false">
      <mvcSiteMapNode title="Log In" action="Login" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="anonymous">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="My Analytics" action="MyAnalytics" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated">
      </mvcSiteMapNode>           
      <mvcSiteMapNode title="Register" action="Register" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated">
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Log Off" action="LogOff" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated">
      </mvcSiteMapNode>
    </mvcSiteMapNode>
  </mvcSiteMapNode>
</mvcSiteMap>
