<?xml version="1.0" encoding="utf-8" ?>
<mvcSiteMap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="http://mvcsitemap.codeplex.com/schemas/MvcSiteMap-File-3.0"
            xsi:schemaLocation="http://mvcsitemap.codeplex.com/schemas/MvcSiteMap-File-3.0 MvcSiteMapSchema.xsd"
            enableLocalization="true">

  <mvcSiteMapNode title="Home" Controller="Home" action="Index" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-home">
    <mvcSiteMapNode title="People" action="People" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-smile" />
    <mvcSiteMapNode title="Products" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-heart" clickable="false">
      <mvcSiteMapNode title="Services" action="Services" controller="Home" changeFrequency="Always" updatePriority="Normal"  imageUrl="icon-briefcase" />
      <mvcSiteMapNode title="Packages" action="Packages" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-tag" />
      <mvcSiteMapNode title="Presentations" action="Presentations" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-calendar" />
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Resources" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bolt" clickable="false">
      <mvcSiteMapNode title="Community" action="Community" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-globe" />
      <mvcSiteMapNode title="Checklists" action="Checklists" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-check-empty" />
      <mvcSiteMapNode title="Links" action="Links" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-external-link-sign" />
      <mvcSiteMapNode title="Analytics" action="Analytics" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bar-chart" />
      <mvcSiteMapNode title="Demo Shop" action="Shop" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-shopping-cart" />
      <mvcSiteMapNode title="SiteMap" action="SiteMap" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-sitemap" />
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Contact" action="Contact" controller="Home" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-bell" />
    <mvcSiteMapNode title="Log In" action="Login" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="anonymous" />
    <mvcSiteMapNode title="Account" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-key" clickable="false" visibility="authenticated">
      <mvcSiteMapNode title="My Analytics" action="MyAnalytics" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated" />
      <mvcSiteMapNode title="AspNet Configuration" action="AspNetConfiguration" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated" />
      <mvcSiteMapNode title="AspNet Paths" action="AspNetPaths" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated" />
      <mvcSiteMapNode title="Register" action="Register" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated" />
      <mvcSiteMapNode title="Log Off" action="LogOff" controller="Account" changeFrequency="Always" updatePriority="Normal" imageUrl="icon-signin" clickable="true" visibility="authenticated" />
    </mvcSiteMapNode>
  </mvcSiteMapNode>
</mvcSiteMap>
